<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="480.0" prefWidth="720.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.mazeduelclient.FriendsController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <AnchorPane prefHeight="440.0" prefWidth="702.0">
      <children>
         <ListView fx:id="listViewFriends" layoutX="216.0" layoutY="54.0" prefHeight="350.0" prefWidth="248.0" />
         <Button fx:id="buttonAll" layoutX="405.0" layoutY="14.0" mnemonicParsing="false" onAction="#loadAll" text="All" />
         <Button fx:id="buttonOnline" disable="true" layoutX="234.0" layoutY="14.0" mnemonicParsing="false" onAction="#loadOnline" text="Online" />
         <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
         <Button layoutX="579.0" layoutY="379.0" mnemonicParsing="false" onAction="#addFriend" text="Add friend" />
         <Button layoutX="579.0" layoutY="343.0" mnemonicParsing="false" text="Remove friend" />
      </children></AnchorPane>
</VBox>
