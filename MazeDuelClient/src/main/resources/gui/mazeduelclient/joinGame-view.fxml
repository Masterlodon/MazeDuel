<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="480.0" prefWidth="720.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.mazeduelclient.JoinGameController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <AnchorPane prefHeight="440.0" prefWidth="702.0">
      <children>
         <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
         <ListView fx:id="listViewGames" layoutX="192.0" layoutY="62.0" prefHeight="364.0" prefWidth="297.0" />
         <Button fx:id="buttonFriends" disable="true" layoutX="192.0" layoutY="14.0" mnemonicParsing="false" onAction="#selectFriends" prefHeight="30.0" prefWidth="117.0" text="Friends">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
         <Button fx:id="buttonPublic" layoutX="378.0" layoutY="14.0" mnemonicParsing="false" onAction="#selectPublic" prefHeight="30.0" prefWidth="111.0" text="Public">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
         <Button layoutX="620.0" layoutY="387.0" mnemonicParsing="false" text="Join">
            <font>
               <Font size="18.0" />
            </font>
         </Button>
      </children>
   </AnchorPane>
</VBox>
