<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="480.0" prefWidth="720.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/18" fx:controller="gui.mazeduelclient.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <AnchorPane prefHeight="440.0" prefWidth="702.0">
         <children>
            <Label layoutX="150.0" layoutY="90.0" text="User name:">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <Label layoutX="150.0" layoutY="168.0" text="Password:">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <TextField fx:id="textFieldUserName" layoutX="324.0" layoutY="90.0" prefHeight="25.0" prefWidth="251.0" />
            <TextField fx:id="textFieldPassword" layoutX="324.0" layoutY="168.0" prefHeight="25.0" prefWidth="251.0" />
            <Button layoutX="215.0" layoutY="269.0" mnemonicParsing="false" onAction="#login" prefHeight="25.0" prefWidth="251.0" text="Login">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
