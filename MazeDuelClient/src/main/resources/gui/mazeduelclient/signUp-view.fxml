<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="480.0" prefWidth="720.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.mazeduelclient.SignUpController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <AnchorPane prefHeight="440.0" prefWidth="702.0">
      <Label layoutX="144.0" layoutY="90.0" text="User name:">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Label layoutX="144.0" layoutY="162.0" text="Password:">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="textFieldUserName" layoutX="324.0" layoutY="90.0" prefHeight="25.0" prefWidth="251.0" />
      <PasswordField fx:id="passwordFieldInput" layoutX="324.0" layoutY="162.0" prefHeight="25.0" prefWidth="251.0" />
      <PasswordField fx:id="passwordFieldConfirm" layoutX="324.0" layoutY="238.0" prefHeight="25.0" prefWidth="251.0" />
      <Button layoutX="215.0" layoutY="320.0" mnemonicParsing="false" onAction="#signUp" prefHeight="25.0" prefWidth="251.0" text="Sign up">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="215.0" layoutY="370.0" mnemonicParsing="false" onAction="#loadSceneLogin" prefHeight="25.0" prefWidth="251.0" text="Login">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label layoutX="144.0" layoutY="238.0" text="Confirm password:">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
   </AnchorPane>
</VBox>
